<div class="row">
	<div class="col-md-4">
		<%= render "users/userinfo" %>
		<%= render "posts/post_form" %>
		<%= render "goals/goalviewindex" %>
	</div>
	<div class="col-md-8">
		<h1>All My Workouts</h1>

		<% if @feed_items.any? %>
		    <%= render partial: 'shared/feed_item', collection: @feed_items.where(:model_name => "workout") %>
		<% end %>
		<%= will_paginate @feed_items %>
	</div>
</div>