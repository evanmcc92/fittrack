<% if current_user %>

	<div class="row">
		<div class="col-md-4">
			<%= render "users/userinfo" %>
			<%= render "posts/post_form" %>
			<%= render "goals/goalviewindex" %>
		</div>
		<div class="col-md-8">
			<%= render 'shared/feed' %>
			<%= will_paginate @feed_items %>
		</div>
	</div>

<% else %>

	<div class="row">
		<div id="index-col-md-8" class="col-md-8">
			<h1>StaticPage#index</h1>
			<p>Find me in app/views/static_page/index.html.erb</p>
		</div>
		<div class="col-md-4"><%= render "devise/sessions/loginform" %></div>
	</div>

<% end %>